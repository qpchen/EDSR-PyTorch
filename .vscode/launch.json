{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/main.py",
            "cwd": "${workspaceFolder}/src",
            "args": [
                "--n_GPUs",
                "1",
                "--scale",
                "2",
                "--patch_size",
                "144",
                "--batch_size",
                "32",
                "--data_test",
                "Set5",
                "--loss",
                "1*SmoothL1",
                "--lr",
                "1e-3",
                "--n_colors",
                "1",
                "--optimizer",
                "ADAM",
                "--skip_threshold",
                "1e6",
                "--epochs",
                "3000",
                "--model",
                "SRARN",
                "--save",
                "srarn/srarn_x2",
                "--reset"
            ],
            "env": {
                "PYTHONUNBUFFERED": "1",
                "CUDA_VISIBLE_DEVICES": "1"
            },
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}