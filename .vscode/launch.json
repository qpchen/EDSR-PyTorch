{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/main.py",
            "cwd": "${workspaceFolder}/src",
            "args": [
                "--n_GPUs",
                "1",
                "--scale",
                "2",
                "--patch_size",
                "128",
                "--batch_size",
                "32",
                "--data_test",
                "Set5+Set14+B100+Urban100+DIV2K",
                "--loss",
                "1*SmoothL1",
                "--lr",
                "1e-3",
                "--n_colors",
                "1",
                "--optimizer",
                "ADAM",
                "--skip_threshold",
                "1e6",
                "--epochs",
                "3000",
                "--model",
                "FSRCNNACV4",
                "--save",
                "fsrcnn_ac_v4_y_sl1_x2_test",
                "--pre_train",
                "../experiment/fsrcnn_ac_v4_y_sl1_x2_test/model/inf_model.pt",
                "--test_only",
                "--save_results",
                "--load_inf"
            ],
            "env": {
                "PYTHONUNBUFFERED": "1",
                "CUDA_VISIBLE_DEVICES": "1"
            },
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}