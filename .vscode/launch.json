{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/main.py",
            "cwd": "${workspaceFolder}/src",
            "args": [
                "--n_GPUs",
                "1",
                "--scale",
                "2",
                "--patch_size",
                "128",
                "--batch_size",
                "32",
                "--data_test",
                "Set5",
                "--loss",
                "1*SmoothL1",
                "--lr",
                "1e-3",
                "--n_colors",
                "1",
                "--optimizer",
                "ADAM",
                "--skip_threshold",
                "1e6",
                "--epochs",
                "3000",
                "--depths",
                // "2+4+2",  //a
                // "2+2+4+2",  //b
                "2+2+6+2",  //c // SwinIR level when training params 612K
                // "3+3+9+3",  //d // SwinIR level
                // "3+3+27+3",  //e // Swin level
                "--dims",
                // "12+24+48",  //1
                // "12+24+48+96",  //2
                "16+32+64+128",  //3 // SwinIR level
                // "24+48+96+192",  //4
                // "48+96+192+384",  //5
                // "96+192+384+768",  //6
                // "128+256+512+1024",  //7 // Swin level
                // "--upsampling",
                // "Nearest",
                // "Deconv",
                // "PixelShuffle",
                "--model",
                "SRARNV2",
                // "--save",
                "--load",
                // "srarn/srarn_v2t_x2",
                // "srarn/srarn_v2s_x2",
                "srarn/srarn_v2c3_x2",
                // "--reset"
                "--resume",
                "-1"
            ],
            "env": {
                "PYTHONUNBUFFERED": "1",
                "CUDA_VISIBLE_DEVICES": "0"
            },
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}